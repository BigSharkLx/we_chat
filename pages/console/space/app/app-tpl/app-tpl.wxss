.index-middle-item {
  position: relative;
  padding: 10px;
  border-bottom: 1px solid #ececec;
}

.item-message {
  margin: 26rpx 0;
  font-size: 30rpx;
  color: #2d2d2d;
}

.item-message-text {
  font-size: 30rpx;
}

.item-img {
  top: 30px;
  position: absolute;
  right: 10px;
  width: 100px;
  max-height: 100px;
}

.app-cpu, .app-storage {
  margin-bottom: 30rpx;
}

.cpu-title {
  color: #333;
  font-size: 34rpx;
  padding: 20rpx;
  border-bottom: 1px solid #efefef;
}

.app-log{
  width: 100%;
  height: 250rpx;
}
.timeline-container{
  padding-bottom: 30rpx;
  display: flex;
  flex-direction: column;
}
.timeline-item {
  max-width: 600rpx;
  word-break: break-all;
  float: left;
  margin-left: 40rpx;
  position: relative;
  padding: 30rpx;
}

.timeline-item::before {
  display: block;
  content: ' ';
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #fff;
  box-shadow: 0 0 5px 3px #36b5de;
  position: absolute;
  top: 46rpx;
  left: -20rpx;
  z-index: 100;
}

.timeline-item::after {
  display: block;
  content: ' ';
  width: 1rpx;
  height:100%;
  background-color: #ddd;
  position: absolute;
  top: 20rpx;
  left: -12rpx;
}
.item-content-date {
  color: #333;
  font-size: 26rpx;
}

.item-content-date {
  color: #333;
  font-size: 26rpx;
}

.item-content-message {
  color: #333;
  font-size: 30rpx;
}
.item-img-love{
  width: 56rpx;
  height: 56rpx;
  vertical-align: middle;
}
.watch-me{
  padding: 10rpx;
  box-shadow: 1px 1px 10rpx #000;
  margin-left: 20rpx;
  border-radius: 10rpx;
}
.activeDataFinished{
  font-size: 26rpx;
  text-align: center;
  margin: 10rpx 0;
  color: #666;
}
.app-watch{
  float: left;
}